<%= render 'notice' %>
<h2>Log in</h2>


<%= form_tag(:action => 'login_attempt') do %>
    <p>Email:</br> <%= text_field_tag(:username) %></p>
    <p>Password:</br> <%= password_field_tag :password %></p>
    <%= submit_tag("Log In") %>

<% end %>

<br>
<p> <%= link_to 'Sign Up', new_user_path%></p>
